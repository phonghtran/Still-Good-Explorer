{
  "name": "app-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "eslint \"src/**/*.{js,vue}\"",
    "lint-fix": " eslint \"src/**/*.{js,vue}\" --fix"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "axios": "latest",
    "bootstrap": "latest",
    "bootstrap-vue": "latest",
    "moment": "latest"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "fibers": "^3.1.1",
    "sass": "^1.17.2",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "rules": {
      "no-undef": 0,
      "vue/html-indent": 1,
      "vue/require-v-for-key": 0,
      "vue/require-default-prop": 0,
      "vue/v-bind-style": 0,
      "vue/no-unused-components": 0,
      "vue/attribute-hyphenation": 0,
      "vue/html-self-closing": 0,
      "vue/name-property-casing": 0,
      "vue/require-prop-types": 0,
      "vue/prop-name-casing": 0,
      "semi": 1,
      "vue/attributes-order": 2
    },
    "plugins": [
      "vue"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "parser": "babel-eslint",
      "sourceType": "module"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
